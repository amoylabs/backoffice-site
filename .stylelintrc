{
	"processors": [
		"stylelint-processor-styled-components"
	],
	"extends": [
		"stylelint-config-recommended",
		"stylelint-config-styled-components"
	],
	"rules": {
		"indentation": "tab",
		"property-no-vendor-prefix": [ true, {
			"ignoreProperties": ["box-orient", "box-shadow"]
		}],
		"value-no-vendor-prefix": [true, {
			"ignoreValues": ["box"]
		}]
	}
}